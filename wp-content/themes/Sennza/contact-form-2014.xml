						<text><![CDATA[5 - 10k]]></text>
					</choice>
					<choice>
						<text><![CDATA[10 - 20k]]></text>
					</choice>
					<choice>
						<text><![CDATA[20 - 50k]]></text>
					</choice>
					<choice>
						<text><![CDATA[50k +]]></text>
					</choice>
					<choice>
						<text><![CDATA[Unsure]]></text>
					</choice>
				</choices>
			</field>
